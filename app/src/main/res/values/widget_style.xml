<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconTouchView">
        <attr name="src_icon_color" format="color" />
        <attr name="dest_icon_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TextTouchView">
        <attr name="src_text_color" format="color" />
        <attr name="dest_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ShapeView">
        <attr name="sv_shape" format="enum">
            <enum name="rectangle" value="0x11" />
            <enum name="oval" value="0x22" />
        </attr>
        <attr name="sv_radius" format="dimension" />
        <attr name="sv_left_top_radius" format="dimension" />
        <attr name="sv_left_bottom_radius" format="dimension" />
        <attr name="sv_right_top_radius" format="dimension" />
        <attr name="sv_right_bottom_radius" format="dimension" />
        <attr name="sv_alpha" format="integer" />
        <attr name="sv_stroke_color" format="color" />
        <attr name="sv_stroke_width" format="dimension" />
        <attr name="sv_stroke_gap" format="dimension" />
        <attr name="sv_stroke_gap_width" format="dimension" />
        <attr name="sv_solid" format="color" />
    </declare-styleable>

    <declare-styleable name="ZoomImageView">
        <attr name="src" format="reference"/>
    </declare-styleable>
</resources>